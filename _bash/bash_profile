export PATH=/usr/local/bin:$PATH
export PATH=/usr/local/heroku/bin:$PATH
export PATH=$PATH:$HOME/.rbenv/shims
export PATH=$PATH:$HOME/.npm/node_modules/bin
export PATH=$PATH:/opt/local/bin


if [ -f ~/.bashrc ]; then
	. ~/.bashrc
fi

if [ -f ~/.aliases ]; then
	. ~/.aliases
fi

eval "$(rbenv init -)"
