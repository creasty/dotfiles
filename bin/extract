#!/usr/bin/env bash

case $1 in
  *.tar)                   tar  xvf $1;;
  *.tar.gz|*.tgz|*.tar.Z)  tar zxvf $1;;
  *.tar.bz2|*.tbz)         tar jxvf $1;;
  *.tar.xz)                tar Jxvf $1;;
  *.gz)                    gzip -dc $1;;
  *.bz2)                   bzip2 -dc $1;;
  *.zip)                   unzip $1;;
  *.lzh)                   lha e $1;;
  *.Z)                     uncompress $1;;
  *.arj)                   unarj $1;;
esac
