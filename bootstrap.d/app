#!/bin/bash

#  Karabiner
#-----------------------------------------------
cp ~/dotfiles/app/karabiner/private.xml ~/Library/Application\ Support/Karabiner/


#  Automator
#-----------------------------------------------
cp ~/dotfiles/app/automator/*.workflow ~/Library/Services/


#  QuickLook
#-----------------------------------------------
